{
	"_version": "1.12.0",
	"sap.app": {
		"id": "moldsawski.movie",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "moldsawski.movie.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "moldsawski.movie.i18n.i18n"
				}
			},
			"movies": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/Movies.json",
				"preload": false
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "moldsawski.movie.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": ["NotFound"]
				}
			},
			"routes": [{
				"name": "Detail",
				"pattern": "movies/{movieId}/appointments/{appointmentId}",
				"titleTarget": "Detail",
				"greedy": false,
				"target": ["Detail"]
			}, {
				"name": "Home",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["Home"]
			}, {
				"name": "Home",
				"pattern": "Home",
				"target": ["Home"]
			}],
			"targets": {
				"Detail": {
					"viewType": "XML",
					"viewName": "Detail"
				},
				"Home": {
					"viewType": "XML",
					"viewName": "Home"
				},
				"NotFound": {
					"viewType": "XML",
					"viewName": "NotFound"
				}
			}
		}
	}
}